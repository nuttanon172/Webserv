server
{
    listen 8002;
    listen 9003

	server_name localhost;
    root docs/fusion_web/;
    client_max_body_size 0;
	index index.html abc.html;

    location / 
    {
        allow_methods GET DELETE POST;
        autoindex off;
    }

    location /tours 
    {
        autoindex on;
        index tours1.html abc.html index.html;
        allow_methods POST;
    }

    location /tours/pass
    {
        index index.html
        allow_methods GET;
    }

	location /red 
    {
		return 303 /tours;
	}

    location /cgi 
    {
        root ./;
        allow_methods POST GET DELETE;
        index index.php;
        cgi_path /usr/bin/php-cgi;
        cgi_ext .php;
    }
}

server 
{
    listen 8004;
    server_name localhost
    root docs/fusion_web2/;
    error_page 404 error_page/404.html;

    location /tours 
    {
        autoindex on;
    }

    location /tours/test
    {
        autoindex on;
    }
}

server 
